# .env - ИСПРАВЛЕННАЯ ВЕРСИЯ
# Database
DB__DB_HOST=localhost
DB__DB_PORT=5432
DB__DB_NAME=aio_edu
DB__DB_USER=user
DB__DB_PASSWORD=password

# MinIO (S3 compatible storage)
MINIO__MINIO_ENDPOINT=localhost:9000
MINIO__MINIO_ACCESS_KEY=minioadmin
MINIO__MINIO_SECRET_KEY=minioadmin_strong_password
MINIO__MINIO_SECURE=false
MINIO__MINIO_BUCKET_NAME=aio-edu

# AI Service
AI__AI_API_BASE=http://localhost:8000/v1
AI__AI_DEFAULT_MODEL=local-model
AI__AI_TIMEOUT=60

# Security
SECURITY__JWT_SECRET_KEY=your_super_strong_jwt_secret_key_512_bits_here
SECURITY__JWT_ALGORITHM=HS256
SECURITY__JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
SECURITY__JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Telegram Bot (ВАЖНО: правильный префикс)
TELEGRAM__BOT_TOKEN=your_bot_token_here
TELEGRAM__WEBHOOK_URL=
TELEGRAM__ALLOWED_CHAT_IDS=

# PDF Processing (ВАЖНО: правильный префикс)
PDF__TEMP_DIR=/tmp/pdf_processing

# Vector Search (ВАЖНО: правильный префикс)
VECTOR__DIMENSION=1536
VECTOR__INDEX_TYPE=hnsw

# RabbitMQ (ВАЖНО: правильный префикс)
RABBITMQ__HOST=rabbitmq
RABBITMQ__PORT=5672
RABBITMQ__USER=guest
RABBITMQ__PASSWORD=guest

# Rate Limits (ВАЖНО: правильный префикс)
RATE_LIMITS__COURSE_GENERATION_LIMIT=1
RATE_LIMITS__COURSE_GENERATION_RESET_DAYS=7
RATE_LIMITS__AI_EXPLANATION_LIMIT=50
RATE_LIMITS__AI_EXPLANATION_RESET_HOURS=24

# Application
DEBUG=true
CORS_ORIGINS=["http://localhost:5137", "http://localhost:3000"]